        dpmi32.c    \
        dpmiint.c   \
        dpmiselr.c  \
        buffer.c    \
        data.c      \
        int21map.c  \
        modesw.c    \
        register.c  \
        savestat.c  \
        stack.c     \
        vxd.c       \
        xmem.c

X86_SOURCES=x86\dpmi386.c   \
            x86\dpmimem.c   \
            x86\dpmimisc.c

MIPS_SOURCES=dpmimemr.c dpmimscr.c
PPC_SOURCES=dpmimemr.c dpmimscr.c
ALPHA_SOURCES=dpmimemr.c dpmimscr.c

